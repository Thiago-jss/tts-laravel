APP_NAME="TTS Laravel"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost:8000

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Database: usando SQLite para desenvolvimento local (zero-config)
# Em produção, trocar para mysql/pgsql e configurar DB_HOST, DB_PORT, DB_DATABASE, DB_USERNAME, DB_PASSWORD
DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=local
QUEUE_CONNECTION=sync
SESSION_DRIVER=file
SESSION_LIFETIME=120

# ElevenLabs API Configuration
# Obtenha sua chave em: https://elevenlabs.io/app/settings/api-keys
# IMPORTANTE: NÃO commite chaves reais! Use secrets no CI/CD.
ELEVEN_API_KEY=your_elevenlabs_api_key_here
ELEVEN_VOICE_ID=21m00Tcm4TlvDq8ikWAM
# Voice IDs disponíveis: https://api.elevenlabs.io/v1/voices
# 21m00Tcm4TlvDq8ikWAM = Rachel (English, Female)
# Outros: EXAVITQu4vr4xnSDxMaL = Bella, ErXwobaYiN019PkySvjV = Antoni

# ElevenLabs API Base URL
ELEVEN_API_BASE_URL=https://api.elevenlabs.io/v1

# Timeout para requests HTTP (segundos)
ELEVEN_API_TIMEOUT=30

MAIL_MAILER=smtp
MAIL_HOST=mailpit
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

VITE_APP_NAME="${APP_NAME}"
